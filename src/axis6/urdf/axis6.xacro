<?xml version="1.0"?>
<!-- Revolute-Revolute Manipulator -->
<robot name="axis6" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Constants for robot dimensions -->
  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="mass" value="1" /> <!-- arbitrary value for mass -->

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find axis6)/urdf/axis6.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find axis6)/urdf/materials.xacro" />

  <!-- Used for fixing robot to Gazebo 'base_link' -->
  <link name="world"/>

  <joint name="joint0" type="continuous">
    <parent link="world"/>
    <child link="link1"/>
    <origin xyz="0 0 2" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.01"/>
  </joint>

  <link name="link1">
    <collision>
      <origin xyz="0 0 0.275" rpy="0 0 0"/>
      <geometry>
	      <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0.275" rpy="0 0 0"/>
      <geometry>
	      <box size="0.1 0.1 0.55"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0.275" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
	      ixx="0.02604166666666667" ixy="0.0" ixz="0.0"
	      iyy="0.02604166666666667" iyz="0.0"
	      izz="0.001666666666666667"/>
    </inertial>
  </link>

  <joint name="joint1" type="continuous">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.5" rpy="${PI/2} 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.01"/>
  </joint>

  <link name="link2">
    <collision>
      <origin xyz="0.4 0 0.1" rpy="0 0 0"/>
      <geometry>
	      <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0.4 0 0.1" rpy="0 0 0"/>
      <geometry>
	      <box size="0.9 0.1 0.1"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <inertial>
      <origin xyz="0.4 0 0.1" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
	      ixx="0.001666666666666667" ixy="0.0" ixz="0.0"
	      iyy="0.06833333333333333" iyz="0.0"
	      izz="0.06833333333333333"/>
    </inertial>
  </link>

  <joint name="joint2" type="continuous">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0.8 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.01"/>
  </joint>

  <link name="link3">
    <collision>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <geometry>
	      <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <geometry>
	      <box size="0.1 0.1 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
	      ixx="0.001666666666666667" ixy="0.0" ixz="0.0"
	      iyy="0.001666666666666667" iyz="0.0"
	      izz="0.001666666666666667"/>
    </inertial>
  </link>
  <joint name="joint3" type="continuous">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0 0 0.1" rpy="${PI/2} 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.01"/>
  </joint>

  <link name="link4">
    <collision>
      <origin xyz="0 0 0.4" rpy="0 0 0"/>
      <geometry>
	       <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0.4" rpy="0 0 0"/>
      <geometry>
	       <box size="0.1 0.1 0.9"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0.4" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
	      ixx="0.06833333333333333" ixy="0.0" ixz="0.0"
	      iyy="0.06833333333333333" iyz="0.0"
	      izz="0.001666666666666667"/>
    </inertial>
  </link>

  <joint name="joint4" type="continuous">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0 0 0.8" rpy="${PI/2} 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.01"/>
  </joint>

  <link name="link5">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	      <cylinder length="0.1" radius="0.0002"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	      <cylinder length="0.1" radius="0.05"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
	      ixx="0.0014583333333333334" ixy="0.0" ixz="0.0"
	      iyy="0.0014583333333333334" iyz="0.0"
	      izz="0.0012500000000000002"/>
    </inertial>
  </link>

  <joint name="joint5" type="continuous">
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz="0 -0.2 0" rpy="${PI/2} 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.01"/>
  </joint>

  <link name="link6">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	      <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	      <box size="0.1 0.1 0.5"/>
      </geometry>
      <material name="white"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
	      ixx="0.021666666666666667" ixy="0.0" ixz="0.0"
	      iyy="0.021666666666666667" iyz="0.0"
	      izz="0.001666666666666667"/>
    </inertial>
  </link>
  <!--Finished-->

  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint1">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

</robot>
